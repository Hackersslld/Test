-- Boot the Library
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- Define the allowed Roblox IDs directly in the script
local allowedRobloxIDs = {
    "123456789", -- Replace with actual allowed IDs
    "987654321",
    "135792468"
}

-- Function to check if the current userâ€™s Roblox ID is allowed
local function isUserAllowed()
    local player = game.Players.LocalPlayer
    if player then
        return table.find(allowedRobloxIDs, tostring(player.UserId)) ~= nil
    end
    return false
end

-- Define a more complex key (make sure this key is kept secret)
local complexKey = "U7fS2@i3LpR9!vB6" -- Replace with your complex key

-- Create the Main Window
local Window = Rayfield:CreateWindow({
   Name = "OxyHub",
   LoadingTitle = "Loading OxyHub",
   LoadingSubtitle = "Scripting Made Easy",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = "OxyHub", -- Custom folder name
      FileName = "Config"
   },
   Discord = {
      Enabled = false,
      Invite = "noinvitelink",
      RememberJoins = true
   },
   KeySystem = true,
   KeySettings = {
      Title = "Access Key",
      Subtitle = "Enter your key",
      Note = "Get your key from our website",
      FileName = "Key",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {complexKey}, -- Use the complex key here
      Callback = function(enteredKey)
         if enteredKey == complexKey and isUserAllowed() then
            print("Key is correct and user is allowed. Loading scripts...")
            -- Code to load the scripts and UI
         else
            print("Invalid key or user not allowed. Please check your key and access permissions.")
            -- Prevent further execution
            return
         end
      end
   }
})

-- Create Tabs for Different Sections
local HomeTab = Window:CreateTab("Home", 4483362458)
local ScriptsTab = Window:CreateTab("Scripts", 4483362458)
local NewsTab = Window:CreateTab("News", 4483362458)
local DiscordTab = Window:CreateTab("Discord", 4483362458)
local UpdatesTab = Window:CreateTab("Updates", 4483362458)

-- Add Sections and Elements to Home Tab
HomeTab:CreateLabel("Welcome to OxyHub!")
HomeTab:CreateLabel("News: Check out the latest updates!")
HomeTab:CreateSection("Server Status")
HomeTab:CreateLabel("Server 1: Online - Running on version 1.2.3 with 99.9% uptime.")
HomeTab:CreateLabel("Server 2: Online - Backup server with version 1.2.3 and 100% uptime.")
HomeTab:CreateLabel("Database: Online - Database server running smoothly with 100% uptime.")
HomeTab:CreateSection("Other")
HomeTab:CreateButton({
    Name = "GitHub",
    Callback = function()
        -- Open GitHub repository link
        print("Opening GitHub...")
    end
})

-- Add Sections and Elements to Scripts Tab
ScriptsTab:CreateButton({
    Name = "Adopt Me!",
    Callback = function()
        if isUserAllowed() then
            loadstring(game:HttpGet("https://egorikusa.space/271ee1956d4b196c73150e64.lua", true))()
        else
            print("Access denied.")
        end
    end
})
ScriptsTab:CreateButton({
    Name = "Bloxburg",
    Callback = function()
        if isUserAllowed() then
            loadstring(game:HttpGet("https://vedrox.com/data/scripts/free/script.lua"))()
        else
            print("Access denied.")
        end
    end
})
